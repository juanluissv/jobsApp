{
  "name": "techo-hiring-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon | pino-pretty",
    "build": "yarn clean && tsc && yarn copy-files",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test mocha --config test/.mocharc.yml",
    "mocha": "mocha --config test/.mocharc.yml",
    "lint": "eslint -c .eslintrc.js \"src/**/*.{js,ts}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"src/**/*.{js,ts}\" --fix",
    "prettier": "prettier -c -w \"src/**/*.{js,ts}\"",
    "db:erd": "ts-node ./src/db/drawErd.ts",
    "db:makemigrations": "cross-env PWD=\"\" node ./node_modules/sequelize-auto-migrations/bin/makemigration.js --models-path ./src/db/index.ts --migrations-path ./src/db/migrations",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "clean": "rimraf ./dist",
    "copy-files": "copyfiles -u 1 src/**/*.{yml,json,png,html,css,js,map} dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/pino": "^6.3.6",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-erd": "^1.3.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "4.1.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.15.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.22.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "express-openapi-validator": "^4.11.0",
    "express-pino-logger": "^6.0.0",
    "faker": "^5.5.3",
    "jwks-rsa": "^2.0.3",
    "pg": "^8.5.1",
    "pino": "^6.11.1",
    "sequelize": "^6.5.0",
    "sequelize-auto-migrations": "github:scimonster/sequelize-auto-migrations#a063aa6535a3f580623581bf866cef2d609531ba",
    "swagger-jsdoc": "6.0.0",
    "swagger-ui-express": "^4.1.6"
  }
}
